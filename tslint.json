{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "prettier": [true, { "singleQuote": true, "semi": false }],
    "interface-name": false,
    "no-submodule-imports": false,
    "no-console": false,
    "variable-name": false
  }
}
